<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
http://www.springframework.org/schema/beans ">

	<bean id="user" class="entities.User">
		<property name="name" value="mary"/>
		<property name="id" value="9"/>
		<property name="email" value="rtjekgneklrgj"/>
		<property name="birthday" value="2000"/>
	</bean>

	<bean id="ad" class="entities.Advertisement">
		<property name="title" value="Siamese kitten"/>
		<property name="price" value="789"/>
		<property name="userId" value="1"/>
	</bean>

</beans>
